{
    "name": "alizada/nova-gridder",
    "description": "A Laravel Nova Package for resource details grids.",
    "keywords": [
        "laravel",
        "nova",
        "package",
        "grids"
    ],
    "authors": [
        {
            "name": "Shahriyar Alizada",
            "email": "info@development.az"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Demency\\NovaGridder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Demency\\NovaGridder\\NovaGridderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
